{
  "version": "1.0",
  "name": "POS System Comparison Dashboard",
  "description": "Automated dashboard comparing Ginesis, Logic, and Wondersoft POS systems",
  "created": "2025-12-11T11:39:28.801930",
  "tables": {
    "CostAnalysis": {
      "source": "Cost_Analysis.xlsx",
      "rows": 18,
      "columns": [
        "Cost_ID",
        "SystemName",
        "CostCategory",
        "Year1",
        "Year2",
        "Year3",
        "TotalCost_3Years",
        "Notes"
      ]
    },
    "POSSystemsComparison": {
      "source": "POS_Systems_Comparison.xlsx",
      "rows": 3,
      "columns": [
        "System_ID",
        "SystemName",
        "Vendor",
        "PricingModel",
        "MonthlyFee",
        "SetupCost",
        "HardwareCost",
        "OverallScore",
        "EaseOfUse",
        "CustomizationScore",
        "IntegrationScore",
        "SupportRating"
      ]
    },
    "POSFeaturesMatrix": {
      "source": "POS_Features_Matrix.xlsx",
      "rows": 30,
      "columns": [
        "Feature_ID",
        "SystemName",
        "FeatureCategory",
        "FeatureName",
        "Supported",
        "Rating",
        "Notes"
      ]
    },
    "EyewearWorkflowSteps": {
      "source": "Eyewear_Workflow_Steps.xlsx",
      "rows": 21,
      "columns": [
        "Step_ID",
        "StepNumber",
        "StepName",
        "Description",
        "SystemName",
        "TimeToComplete_Seconds",
        "ErrorRate_Percent",
        "StaffTrainingRequired_Hours"
      ]
    },
    "IntegrationRequirements": {
      "source": "Integration_Requirements.xlsx",
      "rows": 12,
      "columns": [
        "Requirement_ID",
        "SystemName",
        "IntegrationType",
        "RequirementName",
        "Complexity",
        "DevelopmentDays",
        "Cost_USD",
        "Status",
        "Priority"
      ]
    }
  },
  "relationships": [
    {
      "from": "POSSystemsComparison[SystemName]",
      "to": "POSFeaturesMatrix[SystemName]",
      "cardinality": "OneToMany"
    },
    {
      "from": "POSSystemsComparison[SystemName]",
      "to": "EyewearWorkflowSteps[SystemName]",
      "cardinality": "OneToMany"
    },
    {
      "from": "POSSystemsComparison[SystemName]",
      "to": "IntegrationRequirements[SystemName]",
      "cardinality": "OneToMany"
    },
    {
      "from": "POSSystemsComparison[SystemName]",
      "to": "CostAnalysis[SystemName]",
      "cardinality": "OneToMany"
    }
  ],
  "measures": {
    "OverallScore": "\n    OverallScore = \n    VAR EaseOfUse = AVERAGE(POS_Systems_Comparison[EaseOfUse])\n    VAR Customization = AVERAGE(POS_Systems_Comparison[CustomizationScore])\n    VAR Integration = AVERAGE(POS_Systems_Comparison[IntegrationScore])\n    VAR Support = AVERAGE(POS_Systems_Comparison[SupportRating])\n    RETURN\n        (EaseOfUse * 0.25) +\n        (Customization * 0.20) +\n        (Integration * 0.35) +\n        (Support * 0.20)\n    ",
    "TCO_3Years": "\n    TCO_3Years = \n    SUMX(\n        Cost_Analysis,\n        Cost_Analysis[Year1] + Cost_Analysis[Year2] + Cost_Analysis[Year3]\n    )\n    ",
    "FeatureCoverage": "\n    FeatureCoverage = \n    DIVIDE(\n        COUNTROWS(\n            FILTER(\n                POS_Features_Matrix,\n                POS_Features_Matrix[Supported] = \"Yes\"\n            )\n        ),\n        COUNTROWS(POS_Features_Matrix),\n        0\n    ) * 100\n    ",
    "IntegrationReadiness": "\n    IntegrationReadiness = \n    VAR AvgComplexity = AVERAGE(Integration_Requirements[Complexity_Numeric])\n    VAR AvgDays = AVERAGE(Integration_Requirements[DevelopmentDays])\n    RETURN\n        10 - ((AvgComplexity * AvgDays) / 10)\n    ",
    "WorkflowEfficiency": "\n    WorkflowEfficiency = \n    VAR AvgTime = AVERAGE(Eyewear_Workflow_Steps[TimeToComplete_Seconds])\n    VAR AvgError = AVERAGE(Eyewear_Workflow_Steps[ErrorRate_Percent])\n    RETURN\n        DIVIDE(1, (AvgTime * AvgError), 0) * 10000\n    ",
    "TotalIntegrationCost": "\n    TotalIntegrationCost = SUM(Integration_Requirements[Cost_USD])\n    ",
    "TotalTrainingHours": "\n    TotalTrainingHours = SUM(Eyewear_Workflow_Steps[StaffTrainingRequired_Hours])\n    ",
    "ErrorRateIndex": "\n    ErrorRateIndex = AVERAGE(Eyewear_Workflow_Steps[ErrorRate_Percent])\n    "
  },
  "pages": [
    {
      "name": "Executive Summary",
      "visuals": [
        {
          "type": "Card",
          "title": "Ginesis Overall Score",
          "measure": "OverallScore",
          "filter": "SystemName = 'Ginesis'",
          "conditional_formatting": {
            ">=8": "Green",
            ">=6": "Yellow",
            "<6": "Red"
          }
        },
        {
          "type": "Card",
          "title": "Logic Overall Score",
          "measure": "OverallScore",
          "filter": "SystemName = 'Logic'",
          "conditional_formatting": {
            ">=8": "Green",
            ">=6": "Yellow",
            "<6": "Red"
          }
        },
        {
          "type": "Card",
          "title": "Wondersoft Overall Score",
          "measure": "OverallScore",
          "filter": "SystemName = 'Wondersoft'",
          "conditional_formatting": {
            ">=8": "Green",
            ">=6": "Yellow",
            "<6": "Red"
          }
        },
        {
          "type": "ColumnChart",
          "title": "3-Year Total Cost of Ownership",
          "x_axis": "SystemName",
          "y_axis": "TCO_3Years",
          "format": "Currency"
        },
        {
          "type": "StackedBar",
          "title": "Feature Coverage",
          "axis": "SystemName",
          "values": "FeatureCoverage",
          "legend": "Supported"
        }
      ]
    }
  ]
}